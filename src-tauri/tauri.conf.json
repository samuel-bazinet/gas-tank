{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../build"
  },
  "package": {
    "productName": "gas-tank",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "gas-tank",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.gas-tank.dev",
      "icon": [
        "icons/gas-tank_32.png",
        "icons/gas-tank_128.png",
        "icons/gas-tank_256.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
